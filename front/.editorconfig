# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

--text: #240f0a;
--background: #f7e7e3;
--primary: #dc2f02;
--secondary: #d4ecf2;
--accent: #3861c2;


[*]
indent_style = space
indent_size = 4
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false